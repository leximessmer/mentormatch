<div class="page-header">
  <h3>Degree #<%= @degree.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>School</dt>
      <dd><%= @degree.school %></dd>

      <dt>Degree</dt>
      <dd><%= @degree.degree %></dd>

      <dt>Graduation date</dt>
      <dd><%= @degree.graduation_date %></dd>

      <dt>User</dt>
      <dd><%= @degree.user_id %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/degrees" class="btn btn-primary">
        Back
      </a>
      <a href="/degrees/<%= @degree.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_degree/<%= @degree.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
